{
  "scripts": {
    "dev": "millennium-ttc --build dev --no-update",
    "sass": "sass ./public/extendium.scss:./public/extendium.css --style compressed --embed-sources --embed-source-map",
    "watch": "nodemon --watch webkit --watch frontend --watch shared --watch public/extendium.scss --ext ts,tsx,js,jsx --exec \"npm run sass && npm run dev\"",
    "build": "npm run sass && millennium-ttc --build prod"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.6",
    "@stylistic/eslint-plugin": "^5.4.0",
    "@types/chrome": "^0.1.24",
    "@types/node": "^24.7.2",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@types/webpack": "^5.28.5",
    "devtools-protocol": "^0.0.1505444",
    "eslint": "^9.37.0",
    "eslint-plugin-perfectionist": "^4.15.1",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.4.0",
    "nodemon": "^3.1.10",
    "semantic-release": "^24.2.9",
    "steam-types": "^1.0.2",
    "typescript-eslint": "^8.46.0"
  },
  "type": "module",
  "name": "extendium",
  "version": "1.0.0",
  "description": "A plugin for Millennium that adds chrome extensions support to the client",
  "main": "./frontend/index.jsx",
  "author": "BossSloth",
  "license": "MIT",
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@steambrew/api": "^5.6.0",
    "@steambrew/client": "^5.6.0",
    "@steambrew/ttc": "^2.7.5",
    "@steambrew/webkit": "5.5.13",
    "react-icons": "^5.5.0",
    "sass-embedded": "^1.93.2",
    "zustand": "^5.0.8"
  },
  "patchedDependencies": {
    "@steambrew/client@5.8.0": "patches/@steambrew%2Fclient@5.8.0.patch"
  }
}
